{
  "name": "train-ticket-machine",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vite build --base ./ --sourcemap true --assetsDir .",
    "test": "yarn test:unit && yarn test:e2e",
    "test:unit": "jest",
    "test:e2e": "cypress run",
    "lint": "yarn lint:scripts && yarn lint:styles",
    "lint:scripts": "eslint --ext .ts,.tsx,.vue ./src",
    "lint:styles": "stylelint  \"./src/**/*.css\" ",
    "cypress:open": "cypress open",
    "build-to-branch": "node ./scripts/build-to-branch.js"
  },
  "dependencies": {
    "lodash-es": "^4.17.15",
    "tailwindcss": "^1.9.6",
    "vue": "^3.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/preset-env": "^7.12.7",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@types/jest": "^26.0.16",
    "@types/lodash-es": "^4.17.3",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "@vue/cli-plugin-unit-jest": "^4.5.9",
    "@vue/compiler-sfc": "^3.0.2",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^5.0.2",
    "@vue/test-utils": "^2.0.0-beta.10",
    "autoprefixer": "^9.0.2",
    "cypress": "^6.0.0",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-vue": "^7.0.0-0",
    "execa": "^4.1.0",
    "husky": "^4.2.3",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.2",
    "postcss": "^8.1.10",
    "prettier": "^2.2.0",
    "stylelint": "^13.8.0",
    "stylelint-config-standard": "^20.0.0",
    "typescript": "~4.1.2",
    "vite": "^1.0.0-rc.9",
    "vue-jest": "^5.0.0-alpha.6"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-push": "yarn test:unit"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,vue}": [
      "yarn lint:scripts"
    ],
    "*.css": [
      "yarn lint:styles"
    ]
  }
}
